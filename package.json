    "@types/node": "^13.7.1",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.2.0",
    "parcel-bundler": "^1.12.4",
    "phaser-ce": "^2.9.4",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4",
    "utf-8-validate": "^5.0.2"
